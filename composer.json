{
    // package name
    "name": "howard/Yii2-inline-widget-behavior",

    // package type
    "type": "yii2-extension",

    "description": "Allows to render widgets in page content in Yii2 Framework based projects",
    "keywords": ["yii2", "behaviors", "inline", "widget"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/howardEagle/Yii2-inline-widgets-behavior/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "authors": [
        {
            "name": "Vovchuk Bohdan",
            "email": "vovchuck.bogdan@gmail.com"
        },
        {
            "name": "Elyseev Dmytryi",
            "email": "mail@elisdn.ru"
        }
    ],

    // package dependencies
    "require": {
        "yiisoft/yii2": "*"
    },

    // class autoloading specs
    "autoload": {
        "psr-4": {
            "howard\\behaviors\\iwb\\": ""
        }
    }
}
